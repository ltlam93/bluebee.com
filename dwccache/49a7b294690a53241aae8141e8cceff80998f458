.subject_name+'</td>'+'<td>'+this.subject_credits+'</td>'+'<td>'+this.subject_credit_hour+'</td>'+'<td>'+this.subject_code+'</td>'+'</tr>').hide().fadeIn(100);});var list=result.subject_type;jQuery.each(result.subject_type,function(i,item){$('#subject_type_name').html(item.subject_type_name).hide().fadeIn(100);console.log(item.subject_type_name);});jQuery(".w-tabs").wTabs();$("#content-tabs.w-tabs .w-tabs-section").first().addClass('active');}});},200);}});});});</script>";