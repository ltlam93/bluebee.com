s:1709:"<script type="text/javascript">$(document).ready(function(){$('#loading-image').hide();jQuery("a.faculty").click(function(){var $self=$(this);jQuery.ajax({type:"POST",url:"/BlueBee.com/listOfSubject/facultyInfoView",beforeSend:function(xhr){jQuery('.three-fourths').html('<img src = "/BlueBee.com/themes/classic/assets/img/ajax_loader_blue_128.gif" style = "padding-left:300px; padding-top: 100px;"/>');},success:function(data){var json=data;setTimeout(function(){var faculty_id=$self.attr("faculty-id");jQuery('.three-fourths').html(data).hide().fadeIn(100);jQuery("#detail_faculty.w-tabs").wTabs();jQuery.ajax({type:"POST",url:"/BlueBee.com/listOfSubject/facultyInfo",data:{faculty_id:faculty_id},beforeSend:function(){jQuery('.three-fourths').hide();},success:function(data){jQuery('.three-fourths').show();var result=$.parseJSON(data);jQuery.each(result.teacher_faculty_position,function(key,value){var item=$('<div style="height: 250px">'+'<div class="w-team-member">'+'<div class="w-team-member-h">'+'<div class="w-team-member-image">'+'<a href = "/BlueBee.com/share/teacher'+'/'+this.teacher_id+'/'+updateKey(this.teacher_name)+'">'+'<img src="'+this.teacher_avatar+'" />'+'</a>'+'</div>'+'<div class="w-team-member-meta">'+'<h5 class="w-team-member-name">'+this.teacher_acadamic_title+' '+this.teacher_name+'</h5>'+'<div class="w-team-member-role">'+this.teacher_position+'</div>'+'</div>'+'</div>'+'</div>').hide().fadeIn(100);jQuery('#teacher_lead').append(item);});jQuery.each(result.faculty_data,function(key,value){jQuery('#faculty_name').append(this.faculty_name);jQuery('#research').append(this.faculty_research);jQuery('#faculty_lab').append(this.faculty_lab);});}});},200);}});});});</script>";